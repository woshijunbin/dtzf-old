<?xml version="1.0" encoding="Shift_JIS"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>centerController</servlet-name>
        <servlet-class>io.junwei.servlet.CenterAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>centerController</servlet-name>
        <url-pattern>/centerController</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>modules/base/fyfb.html</welcome-file>
        <welcome-file>modules/base/fyfb.jsp</welcome-file>
    </welcome-file-list>

</web-app>
